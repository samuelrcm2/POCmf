    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "${workspaceFolder}/POCmf/Controler.py",
                "FLASK_ENV": "development",
                "FLASK_DEBUG": "0",
                "FLASK_URL": "http://localhost:4200"
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload"
            ],
            "justMyCode": false,
            "jinja": true,
        }
    ]
}